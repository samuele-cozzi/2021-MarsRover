{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "RoverSettings": {

    "Landing": {
      "Latitude": 2,
      "Longitude": 2,
      "FacingDirection": "N"
    },
    "Distance": 5000
  },
  "IntegrationSettings": {
    "ReadModelConnectionString": "Server=localhost,5433;Database=RoverRM;User Id=sa;Password=Pass@word",
    "RabbitMQConnectionString": "amqp://localhost",
    "RabbitMQQueue": "roverconsumer",
    "RabbitMQPublishExchange": "controlroom",
    "RabbitMQReadExchange": "rover"
  }
}
