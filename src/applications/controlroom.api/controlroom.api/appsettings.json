{
  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5340"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithProcessId",
      "WithThreadId"
    ]
  },
  "AllowedHosts": "*",
  "RoverSettings": {

    "Landing": {
      "Latitude": 2,
      "Longitude": 2,
      "FacingDirection": "N"
    },
    "Distance": 5000
  },
  "IntegrationSettings": {
    "ReadModelConnectionString": "Server=localhost,5433;Database=RoverRM;User Id=sa;Password=Pass@word",
    "RabbitMQConnectionString": "amqp://localhost",
    "RabbitMQQueue": "controlroomconsumer",
    "RabbitMQPublishExchange": "rover",
    "RabbitMQReadExchange": "controlroom"
  }
}
